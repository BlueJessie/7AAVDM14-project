---
---
[
  {% for archive in site.archives %}
    {

      "title"    : "{{ archive.title }}",
      "url"      : "{{ site.baseurl }}{{ archive.url }}",
      "tags"     : "{{ archive.tags }}",
      "licence"  : "{{ archive.licence }}",
      "year"     : "{{ archive.date | date: "%Y" }}"

    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]