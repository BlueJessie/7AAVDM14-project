---
[
  {% for item in site.pages %}
  {% if item.layout == 'text' %}
    {
      "title"    : "{{ item.version }}-{{ item.title }}",
      "tags"     : "{{ item.tags }}",
      "version"  : "{{ item.version }}",
      "language" : "{{ item.language }}"

    }{% unless forloop.last %},{% endunless %}
    {% endif %}
    {% endfor %}
]
